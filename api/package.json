{
  "name": "snaplant-api",
  "version": "1.0.0",
  "description": "SnapPlant Azure Functions API with Azure OpenAI integration",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "func start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prestart": "npm run build && func extensions install",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@azure/functions": "^4.5.0",
    "@azure/cosmos": "^4.0.0",
    "@azure/storage-blob": "^12.17.0",
    "openai": "^4.24.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}